extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 Searching for: #{searchingBy}
    .serach__vudeos
        if videos.length === 0
            h5 No Videos Found
        each item in videos
            +videoBlock({
                id: item.id,
                title: item.title,
                views: item.views,
                videoUrl: item.videoUrl
            })